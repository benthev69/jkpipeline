pipeline {
    agent any
    
    stages {
        stage ('hello'){
            steps {
                echo 'Hello world'
            }
        }
        
        stage('Build') {
            steps {
                echo'Dev'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Mise en production'
            }
        }
    }
